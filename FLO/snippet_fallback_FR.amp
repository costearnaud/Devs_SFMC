  <style>
    body {
      padding: 16px;
      background-color: #fafafa;
    }

    * {
      box-sizing: border-box;
    }

    .content {
      text-align: center;
      color: #727272;
      font-size: 14px;
      font-weight: 300;
      line-height: 35px;
      font-weight: bold;
      text-decoration: none;
      margin-top: 20px;
    }

    .margintop15 {
      margin-top: 15px;
    }

    .marginbottom15 {
      margin-bottom: 15px;
    }

    .tags {
      text-align: center;
    }

    .tag {
      position: relative;
      z-index: 1;
      display: inline-block;
      vertical-align: top;
      margin-right: 8px;
      margin-bottom: 8px;
      font-weight: normal;
    }

    .selectable .tag:hover {
      z-index: 2;
    }

    .tag input {
      display: none;
    }

    .tag label {
      position: relative;
      display: block;
      margin: 0;
      padding: 4px 8px;
      border-radius: 4px;
      border: 1px solid;
      font-size: 12px;
      font-weight: bold;
      line-height: 18px;
      outline: none;
      transition: background-color 0.1s;
    }

    .tag .count {
      display: inline-block;
      height: 16px;
      padding: 0 4px;
      border-radius: 4px;
      font-size: 9px;
      line-height: 16px;
      text-align: center;
    }

    .selectable .tag label {
      cursor: pointer;
    }

    .tag:hover label {
      border-color: #d7d7d7;
    }

    .tag label {
      background-color: white;
      border-color: #f0f0f0;
      color: #727272;
    }

    .tag label .count {
      background-color: #f0f0f0;
      color: #727272;
    }

    .tag label .delete {
      background-color: white;
      border-color: #d7d7d7;
    }

    :not(.selectable)>.tag label,
    .selectable .tag input:checked+label {
      background-color: #727272;
      color: #f0f0f0;
    }

    :not(.selectable)>.tag label .count,
    .selectable .tag input:checked+label .count {
      background-color: white;
    }

    :not(.selectable)>.tag label .delete,
    .selectable .tag input:checked+label .delete {
      background-color: #f0f0f0;
    }

    .img {
      width: 10%;
      height: 100px;
    }

    button {
      border: 1px solid #555;
      overflow: hidden;
      color: #555;
      cursor: pointer;
      font-size: 16px;
      font-weight: 300;
      line-height: 35px;
      font-weight: bold;
      text-decoration: none;
      text-transform: uppercase;
      background: transparent;
      margin-top: 20px;
    }

    button:hover {
      border: 1px solid #000;
      overflow: hidden;
      color: #fff;
      cursor: pointer;
      font-size: 16px;
      font-weight: 300;
      line-height: 35px;
      text-decoration: none;
      text-transform: uppercase;
      background: #555;
    }
  </style>

